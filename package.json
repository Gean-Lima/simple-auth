{
  "name": "@nuxtjs/simpleauth",
  "version": "1.1.2",
  "description": "A simple authentication module using token",
  "author": {
    "name": "Gean Lima",
    "email": "geanlimasp12345@gmail.com",
    "url": "https://github.com/Gean-Lima"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Gean-Lima/simple-auth"
  },
  "homepage": "https://github.com/Gean-Lima/simple-auth#readme",
  "bugs": {
    "url": "https://github.com/Gean-Lima/simple-auth/issues"
  },
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/types.d.ts",
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs"
    }
  },
  "main": "./dist/module.cjs",
  "types": "./dist/types.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "prepack": "nuxt-module-build build",
    "dev": "nuxi dev playground",
    "dev:build": "nuxi build playground",
    "dev:prepare": "nuxt-module-build build --stub && nuxt-module-build prepare && nuxi prepare playground",
    "release": "npm run lint && npm run test && npm run prepack && changelogen --release && npm publish && git push --follow-tags",
    "lint": "eslint .",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "test:types": "vue-tsc --noEmit && cd playground && vue-tsc --noEmit"
  },
  "dependencies": {
    "@nuxt/kit": "^3.13.2",
    "axios": "^1.7.7",
    "defu": "^6.1.4"
  },
  "devDependencies": {
    "@nuxt/devtools": "^1.5.0",
    "@nuxt/eslint-config": "^0.5.7",
    "@nuxt/module-builder": "^0.8.4",
    "@nuxt/schema": "^3.13.2",
    "@nuxt/test-utils": "^3.14.2",
    "@pinia/nuxt": "^0.5.5",
    "@types/node": "latest",
    "changelogen": "^0.5.7",
    "eslint": "^9.11.0",
    "nuxt": "^3.13.0",
    "pinia": "^2.2.4",
    "typescript": "latest",
    "vitest": "^2.1.1",
    "vue-tsc": "^2.1.6"
  },
  "overrides": {
    "vue": "latest"
  },
  "publishConfig": {
    "access": "public"
  }
}
